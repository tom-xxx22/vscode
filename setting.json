{
  "window.commandCenter": false,
  "workbench.activityBar.location": "hidden",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.mouseWheelZoom": true,
  "workbench.sideBar.location": "right",
  "editor.minimap.enabled": false,
  "workbench.iconTheme": "vscode-icons",
  "window.menuBarVisibility": "toggle",
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "editor.fontFamily": "Hack Nerd Font",
  "editor.fontLigatures": true,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.cursorBlinking": "smooth",
  "editor.cursorWidth": 2,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.scrollbar.horizontalScrollbarSize": 7,
  "editor.scrollbar.verticalScrollbarSize": 7,
  "editor.codeLensFontFamily": "Hack Nerd Font",
  "editor.experimentalInlineEdit.fontFamily": "Hack Nerd Font",
  "editor.inlayHints.fontFamily": "Hack Nerd Font",
  "editor.inlineSuggest.fontFamily": "Hack Nerd Font",
  "debug.console.fontFamily": "Hack Nerd Font",
  "scm.inputFontFamily": "Hack Nerd Font",
  "notebook.output.fontFamily": "Hack Nerd Font",
  "chat.editor.fontFamily": "Hack Nerd Font",
  "editor.lineHeight": 20,
  "terminal.integrated.tabs.enabled": false,
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.layoutControl.enabled": false,
  "editor.suggest.matchOnWordStartOnly": false,
  "editor.fontWeight": "normal",
  "workbench.startupEditor": "none",
  "editor.fontSize": 13,
  "editor.semanticHighlighting.enabled": true,
  "editor.bracketPairColorization.enabled": false,
  "workbench.editor.showTabs": "none"
}
